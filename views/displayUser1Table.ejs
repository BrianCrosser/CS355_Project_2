<% include header %>

    <div>
    <% if(rs.length > 0) { %>
        <table border="1">
            <tr><th>Username</th></tr>
            <% for (var i=0; rs.length > i; i++) { %>
                <tr>
                 <td><a href="/user1/?user1id=<%= rs[i].UserID %>"> <%= rs[i].Username %></a></td>
                 <td><a href="/user1/edit?user1id=<%= rs[i].UserID %>"> <%= "Edit" %></a></td>
                 <td><a href="/user1/delete?user1id=<%= rs[i].UserID %>"> <%= "Delete" %></a></td>

                </tr>
            <% } %>
        </table>
    <% } else { %>
         No users are entered into the User1 database table.
    <% } %>
    </div>

    <div style="margin-top:10px;">
        <a href="/user1/create">Create New User</a>
    </div>

<% include footer %>