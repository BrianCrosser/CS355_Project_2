<% // create a list of javascript files to have the header load %>
<% var js_include = ['create_profile.js']; %>
<% include header %>

<!-- Note that this is a post -->
<form id="create_profile_form" action="<%= action %>">
    <h1>Create New Profile</h1>
    <table id="login">
        <tr><td>First Name:</td>
            <td><input type="text" id="FirstName" name="FirstName" /></td>
        </tr>
        <tr><td>Last Name:</td>
            <td><input type="text" id="LastName" name="LastName" /></td>
        </tr>
        <tr><td>Email:</td>
            <td><input type="text" id="Email" name="Email" /></td>
        </tr>
    </table>
    <!-- not that we use a button here and not submit -->
   <br>
    <input type="button" id="createProfileBtn" value="Create" />
</form>

<div id="output" style="margin:10px;display:none"></div>

<% include footer %>