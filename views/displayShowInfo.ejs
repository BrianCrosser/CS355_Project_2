<% include header %>
<h1>Show Information</h1>
<% if(rs.length > 0)
{ %>
    Show Name: <%= rs[0].ShowName %> <br />
    Number of Seasons: <%= rs[0].Seasons %> <br />
    Rating: <%= rs[0].Rating %> <br />
    Genre: <%= rs[0].Genre %> <br />
    AirTime: <%= rs[0].AirTime %> <br />

    <br />
<% } else { %>
    No Show matched this Title id <%= showid %>.
<% } %>

<% include footer %>